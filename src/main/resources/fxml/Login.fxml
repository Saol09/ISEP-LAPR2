<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ui.gui.LoginUI">
   <center>
      <GridPane prefHeight="300.0" prefWidth="500.0" BorderPane.alignment="CENTER">
        <columnConstraints>
            <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="1.0" prefWidth="1.0" />
            <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="1.0" prefWidth="1.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="lblLogin" text="Login" GridPane.columnIndex="2">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="txtEmail" onAction="#txtEmail" promptText="User Id/ Email" GridPane.columnIndex="3" GridPane.rowIndex="1" />
            <Label fx:id="lblEmail" text="Enter User Id / Email" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="lblPassword" text="Enter Password" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#btnCancelAction" prefHeight="43.0" prefWidth="78.0" text="Cancel" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button fx:id="btnEnter" mnemonicParsing="false" onAction="#btnEnterAction" prefHeight="48.0" prefWidth="76.0" text="Enter" GridPane.columnIndex="3" GridPane.rowIndex="3" />
            <PasswordField fx:id="txtPassword" onAction="#txtPassword" promptText="Password" GridPane.columnIndex="3" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </center>
</BorderPane>
